{
  "name": "now-test",
  "alias": "now-test-golang-2019.now.sh",
  "version": 2,
  "env": {
    "SECRET": "my secret"
  },
  "builds": [
    { "src": "*.go", "use": "@now/go" }
  ],
  "routes": [
    { "src": "/users/new", "dest": "/new-users.go", "methods": ["POST"] },
    { "src": "/users/(?<id>[0-9]+)", "dest": "/one-user.go?id=$id", "methods": ["GET"] },
    { "src": "/users", "dest": "/all-user.go", "methods": ["GET"] },
    { "src": "/", "dest": "/", "methods": ["GET"] }
  ]
}
