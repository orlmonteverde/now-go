{ "name": "now-test",
  "version": 2,
  "env": {
    "SECRET": "my secret"
  },
  "builds": [
    { "src": "*.go", "use": "@now/go" }
  ],
  "routes": [
    { "src": "users/", "methods": ["GET"], "dest":"all-user.go" },
    { "src": "user/new", "methods": ["POST"], "dest":"new-users.go" },
    { "src": "/", "methods": ["GET"], "dest":"/" }
  ]
}
