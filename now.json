{ "name": "now-test",
  "version": 2,
  "env": {
    "SECRET": "my secret"
  },
  "builds": [
    { "src": "*.go", "use": "@now/go" }
  ],
  "routes": [
    { "src": "user/(?<id>[0-9]*)", "methods": ["POST"], "dest":"new-user.go?id=$id" },
    { "src": "user/", "methods": ["GET"], "dest":"all-users.go" },
    { "src": "/", "methods": ["GET"], "dest":"/" }
  ]
}
